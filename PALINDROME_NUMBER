#include<stdio.h>
#include<conio.h>
void main(){
	/* Check PALINDROME */
	int num,temp,sum=0,r;
	printf("\t This is the program for checking if a number is Palindrome or not \t \t \t \n");
    printf("\n Please enter a number: ");
	scanf("%d",&num);
	temp = num;
	while(num>0){
		r=num%10;
		sum =  (sum*10)+r;
		num = num/10; 
	}
	if(temp==sum){
		printf("Yes %d is Palindrome Number",temp);
	}else{
		printf("No %d is Not Palindrome Number",temp);
    }
}
